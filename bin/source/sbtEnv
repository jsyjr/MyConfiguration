# Dash source to establish a various symbols

SBTFLAGS="-max-exec-minutes 60 -sbscanlog-final-user-patterns cgir_profiling"
RERUNFLAGS="-noretryfailedtests -rerunusing /ws/Bcgir_task.latest_pass"

QUALIFY=/mathworks/devel/sandbox/${USER}/qualify
QUALFAIL=${QUALIFY}_sbruntests/glnxa64/sbtest/failed_testsuites.txt

TESTDIR=${WORKSPACE}_retest
SAVERUN=${TESTDIR}/sbruntests.area
ALLFAILS=${TESTDIR}/all_fail.txt
FAILFAIL=${TESTDIR}/failfail.text
RERUN=${TESTDIR}/rerun.txt
HANG=${TESTDIR}/hang.txt
NO_MEM=${TESTDIR}/no_mem.txt

if [ -z "${WSNAME}" ]; then
    echo Required workspace name is missing.
    exit 1
elif [ ! -d ${WORKSPACE} ] ; then
    echo Workspace "${WORKSPACE}" does not exist.
    exit 1
fi
