# Dash source - preform an initial qualification run on the network

echo Testing "${WORKSPACE}".

( cd ${WORKSPACE} \
; ln -Tsf ${WORKSPACE} ${QUALIFY}/COPIED_FROM \
; sbcopyandrun -to-dst ${QUALIFY} -num-threads 16 -no-perforce -- -autofarm devel -lockup-minutes 20 -sbscanlog-final-user-patterns cgir_profiling -rerunusing jobarchive ${SBRT_FULL_CFG} \
; echo \
; echo Initial testing of ${WORKSPACE} is complete. \
; mkdir -p ${RETEST} \
; cp ${QUALFAIL} ${ALLFAILS} \
; echo Status saved as ${ALLFAILS}. \
)
