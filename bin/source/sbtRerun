# Dash source - rerun non-HANG / non-NO_MEM double failures

( cd ${WORKSPACE} \
; echo 'Rerunning double failures (apart from hang and out of memory cases).' \
; echo \
;           sbruntests -delete-previous-test-log-area -test-log-area ${RERUN}.area  ${SBTFLAGS} ${RERUNFLAGS} -local all -lockup-minutes 10 -cfg ${RERUN}.txt \
; echo \
; echo Retesting of ${WORKSPACE} is complete. \
; echo \
; echo Not retested - no_mem: $(grep --count '[-]runsuite' ${NO_MEM}) \
; echo '  ' sbruntests -delete-previous-test-log-area -test-log-area ${NO_MEM}.area ${SBTFLAGS} ${RERUNFLAGS} -local 4   -lockup-minutes 20 -cfg ${NO_MEM}.txt -mlswitches -no-vmemorylimit \
; echo \
; echo Not retested - hang: '' $(grep --count '[-]runsuite' ${HANG}) \
; echo '  ' sbruntests -delete-previous-test-log-area -test-log-area ${HANG}.area   ${SBTFLAGS} ${RERUNFLAGS} -local 8   -lockup-minutes 40  -cfg ${HANG}.txt \
)
