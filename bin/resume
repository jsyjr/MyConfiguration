#!/bin/sh

echo "##### amake all -f run -DCOMP checkin"
amake all -f run -DCOMP checkin
if [ $? != 0 ]
then
    exit $?
fi
amake all -f run checkin
amake -o all -f run checkin
#
# Real run begins here
#
echo "##### The following must be clean..."
rm -rf  ~/asd/data
echo "##### amake -gcov all -f run checkin"
amake -gcov all -f run checkin
if [ $? != 0 ]
then
    exit $?
fi
echo "##### amake all -f run checkin"
amake all -f run checkin
if [ $? != 0 ]
then
    exit $?
fi
echo "##### amake -o all -f run checkin"
amake -o all -f run checkin
if [ $? != 0 ]
then
    exit $?
fi
