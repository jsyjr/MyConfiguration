#!/bin/dash

# run sbver in every sandbox beneath /ws

( cd /ws \
; for DIR in * ; do \
    [ -d $DIR ] && [ -f $DIR/mw_anchor ] && \
    [ $DIR != "Bcgir_task.latest_pass" ] &&  \
    [ $DIR != "Bcgir_thin.latest_pass" ] &&  \
       ( cd /ws/$DIR \
       ; echo -n "$DIR:  " \
       ; sbver | grep 'SyncFrom:' | sed -e 's|SyncFrom: /*mathworks/devel/||' \
       ) \
  done \
)
