#!/bin/sh

# Stand alone script to remove all genreated artifacts, leaving a
# pristine source tree.

# verify working directory
. ${0%${0##*/}}checkwd

rm -rf \
\
all* \
\
data \
doxy \
source \
rel \
\
obj \
cobj \
gobj \
tobj \

rm -f \
\
acpp \
acproto \
amake \
jmake \
\
am.dmp \
BROWSE \
tags \
TAGS \
\
src/am.dmp \
src/BROWSE \
src/tags \
src/TAGS \
