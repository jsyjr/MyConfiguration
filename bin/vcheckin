#!/bin/sh
#
# Premilinary clutter
# 
rm -rf ~/asd/obj ~/asd/source ~/asd/gobj ~/asd/tobj ~/asd/data
echo "##### amake all -f run -DCOMP checkin"
amake -v all -f run -DCOMP checkin
if [ $? != 0 ]
then
    exit $?
fi
amake -v all -f run checkin
amake -v -o all -f run checkin
#
# Real run begins here
#
echo "##### The following must be clean..."
rm -rf  ~/asd/data
echo "##### amake -gcov all -f run checkin"
amake -v -gcov all -f run checkin
if [ $? != 0 ]
then
    exit $?
fi
echo "##### amake all -f run checkin"
amake -v all -f run checkin
if [ $? != 0 ]
then
    exit $?
fi
echo "##### amake -o all -f run checkin"
amake -v -o all -f run checkin
if [ $? != 0 ]
then
    exit $?
fi
