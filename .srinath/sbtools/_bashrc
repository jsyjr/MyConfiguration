# Some example alias instructions
# If these are enabled they will be used instead of any instructions
# they may mask.  For example, alias rm='rm -i' will mask the rm
# application.  To override the alias instruction use a \ before, ie
# \rm will call the real rm not the alias.

source /hub/share/sbtools/bash_setup.bash

# Interactive operation...
alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias gvim='vim -g'

# Default to human readable figures
alias df='df -h'
alias du='du -h'

alias gotob='putty -ssh srinath@biomimetics.eecs.berkeley.edu &'
alias gotoal='putty -ssh srinath@albinoni.eecs.berkeley.edu &'
alias gotoh='putty -load srinath@home &'
alias open='rundll32 url.dll,FileProtocolHandler'
alias xfig='xfig -specialtext -latexfont -startlatexFont default'

export DARCS_EDITOR=gvim

export CVS_RSH=plink
export CVSEDITOR=vimnobg

# export PS1='\[\e]0;\W\a\]\n\[\e[32m\]\[\e[33m\][\@] \u@\h \w\[\e[0m\]\n$(print_network_sandbox_warning)\$ '
export PS1='\[\e]0;\w\a\]\n\[\e[32m\]\[\e[33m\][\@] \u@\h \w\[\e[0m\]\n$ '

bind '"\C-p": history-search-backward'
bind '"\C-n": history-search-forward'
bind '"[A": history-search-backward'
bind '"[B": history-search-forward'

