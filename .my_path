    export HOME=/jyates
else
    export HOME=/home/jyates
fi

PATH=\
/usr/lib/ccache\
:/usr/local/bin\
:/usr/bin\
:/bin\
:/usr/local/sbin\
:/usr/sbin\
:/sbin\

if [ -d /mathworks ]; then
    export SBTOOLS_ROOT=/hub/share/sbtools

    PATH=\
/usr/local/apps/bat/perl/perl-5.20.2-mw-020/bin\
:${PATH}\
:/usr/local/netbin\
:/hub/share/bin\
